{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:List*",
        "s3:GetObject*",
        "s3:PutObject*",
        "s3:CreateMultipartUpload"
      ],
      "Resource": [
%{ for bucket_arn in s3_arns ~}
        "${bucket_arn}/*",
%{ endfor ~}
        "arn:aws:s3:::aegea-batch-jobs-${AWS_ACCOUNT_ID}",
        "arn:aws:s3:::aegea-batch-jobs-${AWS_ACDocker image (name or name:tag) that will be used for Batch jobs"COUNT_ID}/*",
        "arn:aws:s3:::sfn-wdl-dev",
        "arn:aws:s3:::sfn-wdl-dev/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "cloudwatch:PutMetricData"
      ],
      "Resource": "*"
    }
  ]
}
